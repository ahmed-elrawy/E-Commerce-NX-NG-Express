          ],
          "options": {
            "jestConfig": "apps/user/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "user-e2e": {
      "root": "apps/user-e2e",
      "sourceRoot": "apps/user-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
